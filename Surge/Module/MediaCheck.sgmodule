#!name=Media Check
#!desc=Netflix/Disney+/Youtube Unlock Check
#!system=ios


[Panel]
Netflix Check = script-name=Netflix Check, title="Netflix Check", content="Refresh", update-interval=1
DisneyPlus Check = script-name=Disney+ Check,title="Disney+ Check",style=alert,content="Refresh",update-interval=1800
YoutubePremium Check = script-name=Youtube Premium Check, title="YouTube Premium Check", update-interval=1

[Script]
Netflix Check = type=generic, script-path=https://github.com/ifflagged/Neverflagged/raw/main/Surge/Scripts/NetflixCheck.js, argument=title=Netflix Check
Disney+ Check = type=generic,script-path=https://github.com/ifflagged/Neverflagged/raw/main/Surge/Scripts/Disney%2BCheck.js
Youtube Premium Check = type=generic, script-path=https://github.com/ifflagged/Neverflagged/raw/main/Surge/Scripts/YoutubePremiumCheck.js, argument=title=YouTube Premium Check
