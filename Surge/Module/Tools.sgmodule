^https?:\/\/tfsmy\.chengdu\.gov\.cn\/api\/v5\/core\/version - reject
^https?:\/\/tft-app\.cdtft\.cn\/gateway-customer\/tftapp\/tft-ams\/api\/appAd - reject

[Map Local]
# YouTube
# Maasea
^https?:\/\/[\w-]+\.googlevideo\.com\/initplayback.+&oad data-type=text data=""
 
[MITM]
hostname = %APPEND% boxjs.com, boxjs.net, mb3admin.com, api.revenuecat.com, googlevideo.com, youtubei.googleapis.com, www.zhihu.com, zhuanlan.zhihu.com, elemecdn.com, rsscc.com, m.jd.com, api.mcd.cn, qunar.com, router.com, walmartmobile.cn, cdtft.cn, chengdu.gov.cn
