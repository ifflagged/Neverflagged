[Rewrite]
# 去广告
^https:\/\/api-va\.tiktokv\.com\/api\/ad\/ reject
# 域名解析
^https:\/\/tnc\d+-platform-\w+\.tiktokv\.com\/get_domains\/ reject

[Script]
http-request https?:\/\/.+.(tiktokv|tiktokcdn).com\/.+(region\d?=CN|&mcc_mnc=\d+)  script-path=https://github.com/ifflagged/Neverflagged/raw/main/Loon/Scripts/TikTok.js, timeout=20, tag=TikTok

[MITM]
hostname = *.byteoversea.com, *.tik-tokapi.com, *.tiktokv.com, *.tiktokcdn.com
